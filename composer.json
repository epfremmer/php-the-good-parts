{
    "name": "epfremme/php-the-good-parts",
    "type": "library",
    "description": "Satirical prototypical javascript PHP implementation",
    "license": "MIT",
    "authors": [
        {
            "name": "Edward Pfremmer",
            "email": "edward.pfremmer@nerdery.com"
        },
        {
            "name": "Nate Brunette",
            "email": "n@tebru.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Epfremme\\PHP\\TheGoodParts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Epfremme\\PHP\\TheGoodParts\\Tests\\": "tests/"
        },
        "files": [
            "tests/_mocks/classes.php"
        ]
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.5",
        "satooshi/php-coveralls": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
